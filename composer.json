{
    "name": "fireboostio/fireboost-php-sdk",
    "description": "PHP SDK for FireBoost API",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Bodor Szilard",
            "email": "szilard@fireboost.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "fireboostio/fireboost-php-client": "^0.0",
        "fireboostio/fireboost-php-auth": "^0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "FireboostIO\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FireboostIO\\SDK\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
